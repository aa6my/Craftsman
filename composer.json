{
    "name": "dsv/craftsman",
    "description": "Craftsman is the name of the command-line interface that you needed in Codeigniter. It provides a number of helpful commands for your use while developing your application. It is driven by the powerful Symfony Console component.",
    "license": "MIT",
    "keywords": ["syseidev","codeigniter","codeigniter-craftsman","craftsman","cli-craftsman","cli","cli-codeigniter"],
    "authors": [
        {
            "name": "David Sosa Valdes",
            "email": "david.sosa.valdes@gmail.com"
        }
    ],
    "repositories": {
        "dsv": {
            "type": "codeigniter-module",
            "package": {
                "name": "dsv/hmvc-migration",
                "version": "1.0.0",
                "source": {
                    "url": "https://gitlab.com/david-sosa-valdes/CI-HMVC-Migrations",
                    "type": "git",
                    "reference": "7bc90ae7"
                }
            },
            "require":{
                "composer/installers": "*"
            }    
        }
    },    
    "require": {
        "php": ">=5.4.0",
        "symfony/console" : "~2.6",
        "symfony/filesystem" : "2.6.6",
        "twig/twig" : "~1.0",
        "symfony/process": "2.6.6",
        "dsv/hmvc-migration": "1.0.0"
    },    
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "bin": ["craftsman"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
